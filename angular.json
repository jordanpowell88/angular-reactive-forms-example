            ]
          }
        }
      }
    },
    "ui-number-input": {
      "projectType": "library",
      "root": "projects/ui-number-input",
      "sourceRoot": "projects/ui-number-input/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ui-number-input/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ui-number-input/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ui-number-input/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ui-number-input/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
