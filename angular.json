            "tsConfig": "projects/validation-messages/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "ui-select": {
      "projectType": "library",
      "root": "projects/ui-select",
      "sourceRoot": "projects/ui-select/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ui-select/ng-package.json"
          },
          "configurations": {
            "production": {
